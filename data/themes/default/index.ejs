<!DOCTYPE html>
<html>

<head>
  <%- include('partials/head') %>
    <%- locals.theme.head %>
</head>

<body lang="<%- blog.language %>">
  <header>
    <%- include('partials/header'); %>
  </header>

  <div class="entries"> 
    <% posts.forEach(post=> { %>
      <%- include('partials/post-list', {post: post}); %>
    <% }); %>
  </div>

  <a href="/?page=<%- locals.page + 1 %>">Next</a>
  <%- include('partials/author'); %>
  <br><br>

  <!-- Footer -->
  <footer>
    <%- include('partials/footer'); %>
  </footer>
</body>

</html>